	function getEVMColor(percentage)
	{
		if(percentage<=60)
			return 'F50707';
		else if(percentage<=79)
			return 'DCAF36';
		else
			return '36DC48';
	}
	
	function formatNumeric(val)
	{
		if(val == null || val == undefined || val == "")
			val = 0;
			
		return val;
	}
	
	function formatText(val)
	{
		if(val == null || val == undefined || val == "")
			val = " ";
			
		return val;
	}
